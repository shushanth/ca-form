{
  "name": "ca-form",
  "version": "1.0.0",
  "description": "Customer Alliance Form",
  "main": "main.js",
  "scripts": {
    "start": "webpack-dev-server --mode=development --progress --config build/webpack.config.js",
    "build": "rm -rf dist && webpack --config build/webpack.config.js --mode=production",
    "test": "mochapack --webpack-config build/webpack-test.config.js --require src/components/__tests/setup.js src/components/__tests/**/*.spec.js",
    "test:watch": "mochapack --watch --webpack-config build/webpack-test.config.js --require src/components/__tests/setup.js src/components/__tests/**/*.spec.js",
    "clean": "rm -rf node_modules yarn.lock package-lock.json"
  },
  "keywords": [
    "Vue"
  ],
  "author": "shushanth",
  "license": "MIT",
  "dependencies": {
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@vue/test-utils": "^1.0.0-beta.31",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "core-js": "^3.6.4",
    "css-loader": "^3.4.2",
    "eslint": "^5.6.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier-vue": "^2.0.2",
    "eslint-plugin-vue": "^6.2.1",
    "expect": "^25.1.0",
    "html-webpack-plugin": "^3.2.0",
    "jest-serializer-vue": "^2.0.2",
    "jsdom": "^16.2.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^7.0.1",
    "mochapack": "^1.1.13",
    "node-sass": "^4.13.1",
    "prettier": "^1.19.1",
    "regenerator-runtime": "^0.13.3",
    "sass-loader": "^8.0.2",
    "sinon": "^9.0.0",
    "vue-loader": "^15.9.0",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-node-externals": "^1.7.2"
  }
}
